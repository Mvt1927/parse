{
    "name": "parziphal/parse",
    "description": "An Eloquent-like interface for Parse",
    "keywords": ["laravel", "parse", "sdk", "parse-sdk", "eloquent"],
    "license": "MIT",
    "require": {
        "php": ">=8.1",
        "laravel/framework": "^9.0 || ^10.0 || ^11.0",
        "parse/php-sdk": "^2.4"
    },
    "autoload": {
        "psr-4": {
            "Parziphal\\Parse\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Parziphal\\Parse\\ParseServiceProvider"
            ]
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.1",
        "squizlabs/php_codesniffer": "*",
        "phpcompatibility/php-compatibility": "*"
    },
    "config": {
        "allow-plugins": {
            "kylekatarnls/update-helper": true
        }
    }
}
